    "dayjs": "^1.11.13",
    "fuse.js": "^7.1.0",
    "less": "^4.3.0",
    "nuxt": "^3.17.5",
    "nuxtjs-naive-ui": "1.0.2",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "^3.5.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "@unocss/nuxt": "^66.3.2",
    "@vicons/ionicons5": "^0.13.0",
    "naive-ui": "^2.42.0",
    "nuxt-particles": "^0.3.0",
    "release-it": "^19.0.3",
    "unocss": "^66.3.2",
